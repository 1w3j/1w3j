<filetype binary="false" description="Vim dotfiles" name="vimrc">
  <highlighting>
    <options>
      <option name="LINE_COMMENT" value="&quot;" />
      <option name="COMMENT_START" value="" />
      <option name="COMMENT_END" value="" />
      <option name="HEX_PREFIX" value="" />
      <option name="NUM_POSTFIXES" value="" />
      <option name="HAS_BRACES" value="true" />
      <option name="HAS_BRACKETS" value="true" />
      <option name="HAS_PARENS" value="true" />
      <option name="HAS_STRING_ESCAPES" value="true" />
    </options>
    <keywords keywords="Plugin;call;filetype;scriptencoding;set" ignore_case="false" />
    <keywords2 />
    <keywords3 />
    <keywords4 />
  </highlighting>
  <extensionMap>
    <mapping pattern="*vimrc" />
  </extensionMap>
</filetype>
